{
	"name": "vue-google-maps-loader",
	"version": "2.1.3",
	"description": "A Vue 3 composable to dynamically load and reload the Google Maps JavaScript API with localization support.",
	"type": "module",
	"main": "./src/index.js",
	"types": "./src/googleMapsLoader.d.ts",
	"exports": {
		".": {
			"import": "./src/index.js",
			"types": "./src/googleMapsLoader.d.ts"
		}
	},
	"keywords": [
		"vue",
		"vue3",
		"vue3-composition-api",
		"google-maps",
		"google-maps-api",
		"i18n"
	],
	"author": "José Miguel Gonçalves <jose.goncalves@inov.pt>",
	"repository": {
		"type": "git",
		"url": "https://github.com/JoseGoncalves/vue-google-maps-loader.git"
	},
	"license": "Apache-2.0",
	"scripts": {
		"lint": "eslint . --fix",
		"format": "prettier --write ."
	},
	"peerDependencies": {
		"vue": "^3.3.0",
		"vue3-google-map": "^0.25.0"
	},
	"peerDependenciesMeta": {
		"vue3-google-map": {
			"optional": true
		}
	},
	"dependencies": {
		"@googlemaps/js-api-loader": "^2.0.2",
		"@sindresorhus/is": "^7.1.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@vue/eslint-config-prettier": "^10.2.0",
		"eslint": "^9.39.1",
		"eslint-plugin-vue": "^10.6.2",
		"globals": "^16.5.0",
		"prettier": "^3.7.4"
	},
	"files": [
		"src",
		"LICENSE",
		"README.md"
	]
}
